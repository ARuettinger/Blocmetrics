<%= form_for registered_application do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: "form-control", placeholder: "Enter Register Application name here." %>
	</div>
	<div class="form-group">
		<%= f.label :url %>
		<%= f.text_field :url, class: "form-control", placeholder: "Enter Register Application URL here." %>
	</div>
	<%= f.submit "Save", class: 'btn btn-success' %>
<% end %>